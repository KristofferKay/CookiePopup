{
    "manifest_version": 3,
    "name": "Accept Cookies App",
    "version": "1.0",
    "description": "Auto accept cookies",
    "permissions": [
        "activeTab",
        "scripting"
    ],
    "background": {
        "service_worker": "background.js"
    },
    "content_scripts": [
        {
            "matches": [
                "<all_urls>"
            ],
            "js": [
                "content.js"
            ]
        }
    ],
    "action": {
        "default_popup": "popup.html",
        "default_icon": {
            "16": "images/cookie.png"
        }
    }
}